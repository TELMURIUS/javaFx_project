<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="insar_timur.javafx_project.MusicPlayer">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="chooseMusic" text="Choose your music"/>
    <Button text="Select Music" onMouseClicked="#chooseMusic"/>
    <Button text="Play" onMouseClicked="#play"/>
    <Button text="Pause" onMouseClicked="#pause"/>
    <Button text="Stop" onMouseClicked="#stop"/>
    <Button text="Go to Playlist" onMouseClicked="#goToPlaylist"/>
</VBox>
