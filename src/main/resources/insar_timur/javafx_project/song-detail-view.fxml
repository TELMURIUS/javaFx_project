<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml" fx:controller="insar_timur.javafx_project.SongDetailController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <ImageView fx:id="albumCover" fitWidth="200" fitHeight="200"/>
    <Label fx:id="songTitle" text="Song Title"/>
    <Label fx:id="songArtist" text="Artist"/>
    <Label fx:id="songAlbum" text="Album"/>
    <Label fx:id="songGenre" text="Genre"/>
    <Button text="Play" onMouseClicked="#play"/>
    <Button text="Pause" onMouseClicked="#pause"/>
    <Button text="Stop" onMouseClicked="#stop"/>
    <Button text="Back" onMouseClicked="#goBack"/>
</VBox>
